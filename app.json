{
  "expo": {
    "name": "Nirvah Care",
    "slug": "doctorsdesksapp",
    "plugins": [
      "expo-router"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "jsEngine": "jsc",
    "splash": {
      "image": "./assets/images/logoImage.png",
      "resizeMode": "contain",
      "backgroundColor": "#fcfcfc"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doctorsdesks.doctorsdesksapp"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appstore.png",
        "backgroundColor": "#fcfcfc",
        "monochromeImage": "./assets/images/appstore.png"
      },
      "icon": "./assets/images/appstore.png",
      "usesCleartextTraffic": true,
      "package": "com.doctorsdesks.doctorsdesksapp",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appstore.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ba1b6d4f-a871-4c66-b1aa-b673a46bdbbc"
      }
    },
    "owner": "sumitthrita"
  }
}
